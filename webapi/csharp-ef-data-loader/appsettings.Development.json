{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "AghanimsFantasyDatabase": "Host={SQL_HOST};Port=5432;Database=postgres;Username={SQL_USER};Password={SQL_PASSWORD}"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://0.0.0.0:5002",
        "Certificate": {
          "Path": "/https/development.pem",
          "KeyPath": "/https/development.key"
        }
      }
    }
  },
  "DotaWebApi": {
    "BaseUrl": "https://api.steampowered.com/IDOTA2Match_570",
    "EconUrl": "https://api.steampowered.com/IEconDOTA2_570"
  }
}